oopener noreferrer\">\r\n              <img :src=\"item.image\" :title=\"item.text\" />\r\n            </a>\r\n          </div>\r\n        </a-carousel>\r\n      </div>\r\n      <!-- Promo state 3 -->\r\n      <div v-else id=\"promo_3\">\r\n        <div class=\"promo_title\">{{ data.title || 'Sponsor' }}</div>\r\n        <a-button type=\"primary\" ghost @click=\"popupInfo\">{{\r\n          data.btnText || 'Become a Sponsor'\r\n        }}</a-button>\r\n      </div>\r\n    </div>\r\n    <a-divider v-if=\"data\" dashed id=\"reset-margin\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Promo',\r\n  methods: {\r\n    gotoLink() {\r\n      window.open(this.data.link);\r\n    },\r\n    popupInfo() {\r\n      this.$info({\r\n        title: this.data.msgTitle || 'Message Title',\r\n        content: this.data.msgText || 'Put your text here.',\r\n        okText: this.data.msgOkText || 'Ok',\r\n        maskClosable: true\r\n      });\r\n    }\r\n  },\r\n  computed: {\r\n    data() {\r\n      return this.$themeConfig.ads;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n@import '../styles/palette.less';\r\n\r\n.promo {\r\n  padding: 0 1rem 0.6rem;\r\n  font-size: 0.75rem;\r\n  overflow: hidden;\r\n\r\n  #promo_1 {\r\n    cursor: pointer;\r\n\r\n    img {\r\n      float: left;\r\n      margin-right: 1rem;\r\n      max-width: @PromoStyle1MW;\r\n    }\r\n  }\r\n\r\n  #promo_2 {\r\n    img {\r\n      width: 100%;\r\n    }\r\n  }\r\n\r\n  #promo_3 {\r\n    padding: 0 1.5rem;\r\n    margin-bottom: 0.8125rem;\r\n\r\n    .promo_title {\r\n      color: #777;\r\n      font-weight: 300;\r\n      font-size: 0.9375rem;\r\n      margin-bottom: 0.9375rem;\r\n    }\r\n  }\r\n}\r\n\r\n#reset-margin {\r\n  margin: 5px 0;\r\n}\r\n\r\n@media (max-width: @MQMobile) {\r\n  .promo {\r\n    display: @MobileShow;\r\n  }\r\n  #reset-margin {\r\n    display: @MobileShow;\r\n  }\r\n}\r\n</style>\r\n"]}]}